<UserControl x:Class="LeapfrogEditor.BreakabkeBehaviourPropEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LeapfrogEditor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
   <StackPanel Margin="2">
      <local:HeadlineControl Headline="BreakableObject Behaviour Properties" />
      <local:IntEditorControl Headline="Bullet Damage" MyValue="{Binding BreakBulletDamage, Mode=TwoWay}" />
      <local:IntEditorControl Headline="Num of Spawns" MyValue="{Binding NumberOfSpawns, Mode=TwoWay}" />
   </StackPanel>
</UserControl>
